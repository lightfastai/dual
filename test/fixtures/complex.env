# Complex .env file demonstrating godotenv features
# Base configuration
BASE_URL=http://localhost:3000
API_URL=${BASE_URL}/api  # API endpoint
WS_URL=${BASE_URL}/ws    # WebSocket endpoint
ADMIN_URL=$BASE_URL/admin

# Multiline certificate
SSL_CERT="-----BEGIN CERTIFICATE-----
MIIDXTCCAkWgAwIBAgIJAKLdQVPy90WjMA0GCSqGSIb3DQEBCwUAMEUxCzAJBgNV
BAYTAkFVMRMwEQYDVQQIDApTb21lLVN0YXRlMSEwHwYDVQQKDBhJbnRlcm5ldCBX
aWRnaXRzIFB0eSBMdGQwHhcNMjQwMTAxMDAwMDAwWhcNMjUwMTAxMDAwMDAwWjBF
-----END CERTIFICATE-----"

# Private key with multiline value
PRIVATE_KEY='-----BEGIN RSA PRIVATE KEY-----
MIIEpAIBAAKCAQEA1234567890abcdefghijklmnopqrstuvwxyz
ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890abcdefghijklmno
-----END RSA PRIVATE KEY-----'

# Escaped values
MESSAGE="Line 1\nLine 2\nLine 3"
QUOTE_MESSAGE="She said \"Hello\" to the world"
WINDOWS_PATH="C:\\Program Files\\MyApp"

# Complex quotes
JSON_CONFIG='{"port": 3000, "host": "localhost", "ssl": true}'
MIXED_QUOTES="She said 'Hello' to me"
NESTED_JSON='{"users": [{"name": "Alice"}, {"name": "Bob"}]}'

# Inline comments work outside quotes
PORT=3000 # Application port
HOST=localhost # Development host
DEBUG=true # Enable debug mode

# No expansion in single quotes
LITERAL_VAR='${BASE_URL}/static'
LITERAL_PATH='$HOME/documents'

# Values with special characters
COLOR=#FF00FF
CHANNEL=#general
EMAIL=user@example.com
PASSWORD=p@ssw0rd!
REGEX_PATTERN=^[a-zA-Z0-9]+$
MATH_EXPR=2+2=4

# Empty values
EMPTY=
BLANK=""
SINGLE_BLANK=''

# Export statements
export DB_HOST=localhost
export DB_PORT=5432
export DB_NAME=myapp_development

# Variable references and expansion
FULL_DB_URL=postgres://${DB_HOST}:${DB_PORT}/${DB_NAME}
DB_CONNECTION=$DB_HOST:$DB_PORT
APP_NAME="My App - ${NODE_ENV}"

# Unicode and international characters
GREETING="Hello, ‰∏ñÁïå üåç"
EMOJI_STATUS="‚úÖ Ready"
UNICODE_PATH="/path/to/—Ñ–∞–π–ª.txt"